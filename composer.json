{
    "name": "vaersaagod/craft-copy-content",
    "description": "Adds the ability to copy content between sites.",
    "type": "craft-plugin",
    "version": "1.0.0",
    "require": {
        "php": ">=8.0.2",
        "craftcms/cms": "^4.4.0"
    },
    "require-dev": {
        "craftcms/ecs": "dev-main",
        "craftcms/phpstan": "dev-main"
    },
    "autoload": {
        "psr-4": {
            "vaersaagod\\copycontent\\": "src/"
        }
    },
    "extra": {
        "handle": "_copycontent",
        "name": "Copy Content",
        "developer": "Værsågod",
        "documentationUrl": "",
        "class": "vaersaagod\\copycontent\\CopyContent"
    },
    "scripts": {
        "check-cs": "ecs check --ansi",
        "fix-cs": "ecs check --ansi --fix",
        "phpstan": "phpstan --memory-limit=1G"
    },
    "config": {
        "sort-packages": true,
        "platform": {
            "php": "8.0.2"
        },
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "craftcms/plugin-installer": true
        }
    }
}

